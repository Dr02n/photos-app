extends /layout


include /photos/photo-tile

block content
  include /partials/toolbar

  //- main.mdc-toolbar-fixed-adjust.page-main
    .mdc-layout-grid
      .mdc-layout-grid__inner.gallery
        each photo in photos
          .mdc-layout-grid__cell
            +photo-card(photo)

  main.mdc-toolbar-fixed-adjust.page-main
    .mdc-grid-list.mdc-grid-list--twoline-caption.mdc-grid-list--with-icon-align-end
      ul.mdc-grid-list__tiles.gallery
        each photo in photos
          +photo-tile(photo)

  include /partials/footer
